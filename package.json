{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "engines": {
    "node": "16.16.0"
  },
  "description": "",
  "author": "Thomas Le",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start-stand-alone-app:dev":"ts-node -r tsconfig-paths/register ./src/stand-alone.app",
    "start-stand-alone-app:prod":"node dist/stand-alone.app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm:migrate": "typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
    "typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/data-source.ts src/migrations/syncDb",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/data-source.ts",
    "typeorm:create": "typeorm-ts-node-commonjs migration:create",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "9.0.5",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/schedule": "2.2.1",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/throttler": "4.0.0",
    "@nestjs/typeorm": "9.0.1",
    "axios": "1.4.0",
    "bcryptjs": "2.4.3",
    "cache-manager": "5.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.0.3",
    "helmet": "6.1.5",
    "ioredis": "5.3.2",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "mysql2": "3.2.4",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "typeorm": "0.3.15",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@moneteam/eslint-plugin-nestjs": "1.6.0",
    "@nestjs/cli": "9.4.2",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.4.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.194",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "3.0.8",
    "@types/passport-local": "1.0.35",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "46.0.0",
    "jest": "29.3.1",
    "prettier": "2.8.8",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}